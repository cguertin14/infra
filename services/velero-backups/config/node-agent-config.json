{
	"podResources": {
		"cpuRequest": "1500m",
		"cpuLimit": "2500m",
		"memoryRequest": "256Mi",
		"memoryLimit": "1Gi"
	},
	"loadAffinity": [
		{
			"nodeSelector": {
				"matchExpressions": [
					{
						"key": "kubernetes.io/arch",
						"operator": "In",
						"values": [
							"amd64"
						]
					}
				]
			}
		}
	]
}